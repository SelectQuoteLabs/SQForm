{
  "name": "@selectquotelabs/sqform",
  "version": "5.3.0",
  "description": "SQ Form",
  "main": "dist/bundle.cjs.js",
  "types": "dist/index.d.ts",
  "private": false,
  "sideEffects": false,
  "scripts": {
    "test": "jest --watch",
    "build": "rollup --config && tsc",
    "lint": "eslint --ext .js,.ts,.tsx ./src --max-warnings 0",
    "lint.fix": "eslint --ext .js,.ts,.tsx ./src --fix",
    "test.ci": "jest --runInBand",
    "lint.staged": "lint-staged",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "nuke-everything": "rm -rf ./dist && rm -rf ./node_modules && rm -f package-lock.json",
    "commit": "npx git-cz",
    "format": "npx prettier --write 'src/**/*.{js,ts,tsx}'",
    "docs": "cd SQFormDocs && npm start",
    "build-docs": "cd SQFormDocs && npm ci && npm run build"
  },
  "files": [
    "/dist"
  ],
  "commitlint": {
    "rules": {
      "type-enum": [
        2,
        "always",
        [
          "feat",
          "fix",
          "docs",
          "chore",
          "refactor",
          "test",
          "ci"
        ]
      ]
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "prettier --write",
      "npm run lint",
      "git add"
    ],
    "*.{md}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SelectQuoteLabs/SQForm.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "author": "SelectQuoteLabs",
  "license": "MIT",
  "homepage": "https://github.com/SelectQuoteLabs/SQForm#readme",
  "dependencies": {
    "@selectquotelabs/sqhooks": "^2.1.0",
    "@tippy.js/react": "^3.1.1",
    "ag-grid-community": "^25.2.1",
    "ag-grid-react": "^25.2.0",
    "fast-deep-equal": "^3.1.3",
    "formik": "^2.2.6",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "react-datepicker": "^2.13.0",
    "react-text-mask": "^5.4.3",
    "react-weekly-schedule": "0.0.0-development",
    "react-window": "^1.8.5",
    "scplus-shared-components": "https://npm-public.selectquotelabs.com/scplus-shared-components/7.3.1",
    "text-mask-addons": "^3.8.0",
    "use-debounce": "^6.0.0",
    "yup": "^0.32.9"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.46",
    "@material-ui/pickers": "^4.0.0-alpha.11",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.1.2",
    "@semantic-release/npm": "^7.1.1",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@storybook/addon-actions": "^6.3.2",
    "@storybook/addon-controls": "^6.3.2",
    "@storybook/addon-docs": "^6.3.2",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addon-links": "^6.3.2",
    "@storybook/addons": "^6.4.0-alpha.17",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.2",
    "@storybook/testing-react": "0.0.18",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.4",
    "@types/react-text-mask": "^5.4.8",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-react-docgen": "^4.1.0",
    "eslint": "^6.6.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "eslint-plugin-testing-library": "^3.10.2",
    "husky": "^3.1.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.0",
    "rollup": "^2.33.2",
    "rollup-plugin-css-only": "^2.1.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^17.4.2",
    "storybook-addon-material-ui": "^0.9.0-alpha.24",
    "ts-jest": "^26.5.6",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "browserslist": "last 2 Chrome versions",
  "bugs": {
    "url": "https://github.com/SelectQuoteLabs/SQForm/issues"
  },
  "directories": {
    "doc": "docs"
  }
}
